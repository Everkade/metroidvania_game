script/source = "extends Camera2D

func _ready():
	if $Room and $Room/Bounds:
		print(\"!!!\")
		var width = $Room/Bounds.shape.size.x
		var height = $Room/Bounds.shape.size.y
		
		var left = $Room/Bounds.position.x - width / 2
		var right = $Room/Bounds.position.x + width / 2
		var top = $Room/Bounds.position.y - height / 2
		var bottom = $Room/Bounds.position.y + height / 2
		
		limit_left = left
		limit_right = right
		limit_top = top
		limit_bottom = bottom
"

